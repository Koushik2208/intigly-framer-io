{
  "expo": {
    "name": "intigly-frame-io",
    "slug": "intigly-frame-io",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "intiglyframeio",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.koushik2208.intiglyframeio"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-video",
      [
        "expo-font",
        {
          "fonts": ["./assets/fonts/FiraSans-MediumItalic.ttf"],
          "android": {
            "fonts": [
              {
                "fontFamily": "ppneuemontreal",
                "fontDefinitions": [
                  {
                    "path": "./assets/fonts/ppneuemontreal-book.otf",
                    "weight": 400
                  },
                  {
                    "path": "./assets/fonts/ppneuemontreal-medium.otf",
                    "weight": 500
                  }
                ]
              }
            ]
          },
          "ios": {
            "fonts": [
              "./assets/fonts/ppneuemontreal-book.otf",
              "./assets/fonts/ppneuemontreal-medium.otf"
            ]
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "381f1f0e-5829-4b0c-bf93-da30a41349b0"
      }
    }
  }
}
